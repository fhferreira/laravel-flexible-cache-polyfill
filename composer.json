{
    "name": "spatie/laravel-flexible-cache-polyfill",
    "description": "Polyfill for Cache::flexible() in Laravel 10",
    "keywords": [
        "spatie",
        "laravel",
        "cache",
        "flexible",
        "stale-while-revalidate",
        "polyfill"
    ],
    "homepage": "https://github.com/spatie/laravel-flexible-cache-polyfill",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "larastan/larastan": "^2.0",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "predis/predis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\FlexibleCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\FlexibleCache\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\FlexibleCache\\FlexibleCacheServiceProvider"
            ],
            "aliases": {
                "FlexibleCache": "Spatie\\FlexibleCache\\Facades\\FlexibleCache"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
